include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWZNMD

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(BUILDROOT)/dbswznm -I$(BUILDROOT)/wznmd/VecWznm -I$(BUILDROOT)/wznmd/IexWznm -I$(BUILDROOT)/wznmd
INCPATH += -I${WHIZSDKROOT}/build -I${WHIZSDKROOT}/build/apiwzlm -I/opt/homebrew/include -I/opt/homebrew/opt/curl/include -I/opt/homebrew/opt/libgit2/include -I/opt/homebrew/opt/libmicrohttpd/include -I/opt/homebrew/opt/libxml2/include -I/opt/homebrew/opt/openssl@3/include -I/usr/local/mysql/include

OBJS = CrdWznmVec.o PnlWznmVecList.o QryWznmVecList.o PnlWznmVecHeadbar.o PnlWznmVecRec.o PnlWznmVecMNTable.o QryWznmVecMNTable.o PnlWznmVecSrc1NFeed.o QryWznmVecSrc1NFeed.o PnlWznmVecPst1NQuerymod.o QryWznmVecPst1NQuerymod.o PnlWznmVecRef1NPanel.o QryWznmVecRef1NPanel.o PnlWznmVecFct1NTablecol.o QryWznmVecFct1NTablecol.o PnlWznmVecVec1NVectoritem.o QryWznmVecVec1NVectoritem.o PnlWznmVecATitle.o QryWznmVecATitle.o PnlWznmVecDetail.o

LIB = libcrdwznmvec

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)

install:
	mv $(LIB).a $(LIBROOT)/wznmd/

clean:
	rm -f *.o
