# file Makefile
# make file for CrdWznmTbl card job handler, release wznmcmbd_jack
# author Alexander Wirthmueller
# date created: 27 Aug 2020
# modified: 27 Aug 2020

include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWZNMCMBD

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(SRCROOT)/dbswznm -I$(SRCROOT)/wznmcmbd/VecWznm -I$(SRCROOT)/wznmcmbd/IexWznm -I$(SRCROOT)/wznmcmbd
INCPATH += -I/home/mpsitech/src/apiwzlm -I/usr/include -I/usr/include/jsoncpp -I/usr/include/libxml2 -I/usr/include/mysql -I/usr/include/postgresql -I/usr/local/include

OBJS = CrdWznmTbl.o PnlWznmTblRec.o PnlWznmTblDetail.o PnlWznmTblALoadfct.o QryWznmTblALoadfct.o PnlWznmTblATitle.o QryWznmTblATitle.o PnlWznmTbl1NImpexp.o QryWznmTbl1NImpexp.o PnlWznmTbl1NCheck.o QryWznmTbl1NCheck.o PnlWznmTblFr1NRelation.o QryWznmTblFr1NRelation.o PnlWznmTblTo1NRelation.o QryWznmTblTo1NRelation.o PnlWznmTbl1NStub.o QryWznmTbl1NStub.o PnlWznmTbl1NSubset.o QryWznmTbl1NSubset.o PnlWznmTblTbl1NTablecol.o QryWznmTblTbl1NTablecol.o PnlWznmTblRef1NDialog.o QryWznmTblRef1NDialog.o PnlWznmTblRef1NCall.o QryWznmTblRef1NCall.o PnlWznmTblSrc1NFeed.o QryWznmTblSrc1NFeed.o PnlWznmTblRef1NPanel.o QryWznmTblRef1NPanel.o PnlWznmTblFct1NTablecol.o QryWznmTblFct1NTablecol.o PnlWznmTblHk1NVector.o QryWznmTblHk1NVector.o PnlWznmTblRef1NQuerymod.o QryWznmTblRef1NQuerymod.o PnlWznmTblRef1NRtblock.o QryWznmTblRef1NRtblock.o PnlWznmTblPst1NQuerymod.o QryWznmTblPst1NQuerymod.o PnlWznmTblMNVector.o QryWznmTblMNVector.o PnlWznmTblMNQuery.o QryWznmTblMNQuery.o PnlWznmTblHeadbar.o PnlWznmTblList.o QryWznmTblList.o

LIB = libcrdwznmtbl

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wznmcmbd/

clean:
	rm -f *.o

