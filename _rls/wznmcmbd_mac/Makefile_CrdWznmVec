include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWZNMCMBD

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(BUILDROOT)/dbswznm -I$(BUILDROOT)/wznmcmbd/VecWznm -I$(BUILDROOT)/wznmcmbd/IexWznm -I$(BUILDROOT)/wznmcmbd
INCPATH += -I${WHIZSDKROOT}/build -I${WHIZSDKROOT}/build/apiwzlm -I/usr/local/include -I/usr/local/include/curl -I/usr/local/include/libxml2 -I/usr/local/mysql/include

OBJS = CrdWznmVec.o PnlWznmVecRec.o PnlWznmVecDetail.o PnlWznmVecATitle.o QryWznmVecATitle.o PnlWznmVecVec1NVectoritem.o QryWznmVecVec1NVectoritem.o PnlWznmVecSrc1NFeed.o QryWznmVecSrc1NFeed.o PnlWznmVecRef1NPanel.o QryWznmVecRef1NPanel.o PnlWznmVecFct1NTablecol.o QryWznmVecFct1NTablecol.o PnlWznmVecPst1NQuerymod.o QryWznmVecPst1NQuerymod.o PnlWznmVecMNTable.o QryWznmVecMNTable.o PnlWznmVecHeadbar.o PnlWznmVecList.o QryWznmVecList.o

LIB = libcrdwznmvec

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wznmcmbd/

clean:
	rm -f *.o
