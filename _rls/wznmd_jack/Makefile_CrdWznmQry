# file Makefile
# make file for CrdWznmQry card job handler, release wznmd_jack
# author Alexander Wirthmueller
# date created: 27 Aug 2020
# modified: 27 Aug 2020

include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWZNMD

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(SRCROOT)/dbswznm -I$(SRCROOT)/wznmd/VecWznm -I$(SRCROOT)/wznmd/IexWznm -I$(SRCROOT)/wznmd
INCPATH += -I/home/mpsitech/src/apiwzlm -I/usr/include -I/usr/include/jsoncpp -I/usr/include/libxml2 -I/usr/include/mysql -I/usr/include/postgresql -I/usr/local/include

OBJS = CrdWznmQry.o PnlWznmQryRec.o PnlWznmQryDetail.o PnlWznmQryAOrder.o QryWznmQryAOrder.o PnlWznmQryAClause.o QryWznmQryAClause.o PnlWznmQrySup1NQuery.o QryWznmQrySup1NQuery.o PnlWznmQryQry1NQuerycol.o QryWznmQryQry1NQuerycol.o PnlWznmQry1NQuerymod.o QryWznmQry1NQuerymod.o PnlWznmQryMNTable.o QryWznmQryMNTable.o PnlWznmQryMNDialog.o QryWznmQryMNDialog.o PnlWznmQryMNPanel.o QryWznmQryMNPanel.o PnlWznmQryHeadbar.o PnlWznmQryList.o QryWznmQryList.o

LIB = libcrdwznmqry

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wznmd/

clean:
	rm -f *.o

