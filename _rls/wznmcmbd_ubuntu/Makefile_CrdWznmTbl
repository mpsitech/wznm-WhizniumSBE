include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWZNMCMBD

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(BUILDROOT)/dbswznm -I$(BUILDROOT)/wznmcmbd/VecWznm -I$(BUILDROOT)/wznmcmbd/IexWznm -I$(BUILDROOT)/wznmcmbd
INCPATH += -I${WHIZSDKROOT}/build -I${WHIZSDKROOT}/build/apiwzlm -I/usr/include -I/usr/include/jsoncpp -I/usr/include/libxml2 -I/usr/include/mariadb

OBJS = CrdWznmTbl.o PnlWznmTblList.o QryWznmTblList.o PnlWznmTblHeadbar.o PnlWznmTblRec.o PnlWznmTblMNVector.o QryWznmTblMNVector.o PnlWznmTblMNQuery.o QryWznmTblMNQuery.o PnlWznmTblPst1NQuerymod.o QryWznmTblPst1NQuerymod.o PnlWznmTblSrc1NFeed.o QryWznmTblSrc1NFeed.o PnlWznmTblRef1NCall.o QryWznmTblRef1NCall.o PnlWznmTblRef1NDialog.o QryWznmTblRef1NDialog.o PnlWznmTblRef1NPanel.o QryWznmTblRef1NPanel.o PnlWznmTblFct1NTablecol.o QryWznmTblFct1NTablecol.o PnlWznmTblRef1NQuerymod.o QryWznmTblRef1NQuerymod.o PnlWznmTblRef1NRtblock.o QryWznmTblRef1NRtblock.o PnlWznmTblHk1NVector.o QryWznmTblHk1NVector.o PnlWznmTbl1NCheck.o QryWznmTbl1NCheck.o PnlWznmTbl1NImpexp.o QryWznmTbl1NImpexp.o PnlWznmTblTbl1NTablecol.o QryWznmTblTbl1NTablecol.o PnlWznmTbl1NSubset.o QryWznmTbl1NSubset.o PnlWznmTbl1NStub.o QryWznmTbl1NStub.o PnlWznmTblTo1NRelation.o QryWznmTblTo1NRelation.o PnlWznmTblFr1NRelation.o QryWznmTblFr1NRelation.o PnlWznmTblATitle.o QryWznmTblATitle.o PnlWznmTblDetail.o PnlWznmTblALoadfct.o QryWznmTblALoadfct.o

LIB = libcrdwznmtbl

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wznmcmbd/

